<div>
  <h1>Products</h1>
</div>
<div>
  {{#each payload}}
    <div>
      <img src="{{this.thumbnails}}"style="aspect-ratio: 1; width: 200px; object-fit: cover;" />
      <div >{{this.title}}</div>
      <div >{{this.description}}</div>
      <div >{{this.price}}</div>
      <div >{{this.category}}</div>
      <button  onclick={addToCart("{{this._id}}")}>add to cart</button>
    </div>
  {{/each}}
</div>

<div >
  <a href="{{prevLink}}"><button>Anterior</button></a>
  <div>
    {{resPage}}
  </div>
  <a href="{{nextLink}}" ><button>Siguiente</button></a>
</div>
